{
  "name": "sbotics-reduc-support",
  "displayName": "sBotics rEduc Support",
  "description": "Suporte a linguagem rEduc do simulador sBotics",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Luisin13/sbotics-vscode"
  },
  "author": {
    "name": "Luís Otávio",
    "url": "https://github.com/Luisin13"
  },
  "bugs": {
    "url": "https://github.com/Luisin13/sbotics-vscode/issues"
  },
  "publisher": "Luís Otávio",
  "icon": "icons/rEduc.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sboticsR",
        "aliases": [
          "rEduc",
          "sboticsR"
        ],
        "extensions": [
          ".re"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "./icons/rEduc.png",
          "light": "./icons/rEduc.png"
        },
        "filenamePatterns": [
          "*.re"
        ]
      }
    ],
    "grammars": [
      {
        "language": "sboticsR",
        "scopeName": "source.sboticsR",
        "path": "./syntaxes/sboticsR.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "sboticsR",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "devDependencies": {
    "esbuild": "^0.15.10"
  }
}
